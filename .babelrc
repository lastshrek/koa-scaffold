{
  "presets": ["node6"],
  "plugins": [
    "syntax-async-functions",
    "transform-regenerator",
    "add-module-exports",
    "transform-runtime",
    "add-module-exports",
    [
      "transform-es2015-modules-commonjs",
      {
        "allowTopLevelThis": true,
        "strict": false
      }
    ],
    "syntax-trailing-function-commas",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ],
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ]
  ]
}
